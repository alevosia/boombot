        "bufferutil": "^4.0.6",
        "discord-api-types": "^0.31.0",
        "discord-player": "^5.2.2",
        "discord.js": "^13.6.0",
        "erlpack": "github:discord/erlpack",
        "ffmpeg-static": "^5.0.0",
